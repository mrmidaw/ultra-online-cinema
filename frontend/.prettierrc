{
    "trailingComma": "es5",
    "tabWidth": 2,
    "useTabs": true,
    "semi": true,
    "bracketSpacing": true,
    "bracketLine": false,
    "singleQuote": false,
    "importOrder": [
        "<THIRD_PARTY_MODULES>",
        "^@components/(.*)$",
        "^@screens/(.*)$",
        "^@ui/(.*)$",
        "^@hooks/(.*)$",
        "^@shared/(.*)$",
        "^@services/(.*)$",
        "^@assets/(.*)$",
        "^@utils/(.*)$",
        "^@config/(.*)$",
        "^@store/(.*)$",
        "^../(.*)",
        "^./(.*)",
        "(.scss)$"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true
}